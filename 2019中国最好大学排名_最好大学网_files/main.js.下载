/**
 * 验证验证码是否正确
 */
function checkCode(checkCodeVal){
	var isOk = false;
	$.ajax({
		type : "post",
		url : "checkCode.action",
		data :"checkcode="+checkCodeVal,
		async:false, 
		beforeSend : function(){
		},
		success : function(re) {
			if(re=="0"){
				alert("验证码输入不正确！请重新输入");
				window.reload();
			}else{
				isOk = true;
			}
		}
	});	
}